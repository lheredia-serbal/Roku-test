<?xml version="1.0" encoding="utf-8" ?>
<component 
  name="ProgramNode"
  extends="ContentNode"
  xsi:noNamespaceSchemaLocation="https://devtools.web.roku.com/schema/RokuSceneGraph.xsd">
  <interface>
    <field id="key" type="string" />
    <field id="id" type="integer" />
    <field id="title" type="string" />
    <field id="subtitle" type="string" />
    <field id="synopsis" type="string" />
    <field id="categoryName" type="string" />
    <field id="startSeconds" type="integer" />
    <field id="endSeconds" type="integer" />
    <field id="startTime" type="string" />
    <field id="endTime" type="string" />
    <field id="channelName" type="string" />
    <field id="channelCategory" type="string" />
    <field id="formattedDuration" type="string" />
    <field id="durationInMinutes" type="integer" />
    <field id="imageURL" type="string" />
    <field id="programTime" type="string" />
    <field id="channel" type="string" />
    <field id="isNow" type="boolean" value="false" />
    <field id="parentalControl" type="boolean" value="false" />
    <field id="size" type="Array" />
  </interface>
</component>